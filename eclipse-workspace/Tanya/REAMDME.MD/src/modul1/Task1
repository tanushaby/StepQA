package modul1;

public class Task1 {
	public static void main(String[] args) {
        int x;
            x = 9;
        int y;
            y = 3;
        int z;
            z = x / y;
        double d = x % y;
        double c = x *x;
        
        System.out.println("1) " + z);
        System.out.println("2) " + d);
        System.out.println("3) " + c);
        
        
     }

}
